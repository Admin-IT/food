<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.nf.food.food.label.dao.CookTimeDao">
       <resultMap id="cooktime" type="edu.nf.food.food.label.entity.CookTime">
           <id property="id" column="menu_cooking_time_id"/>
           <result property="name" column="menu_cooking_time"/>
       </resultMap>
    <insert id="addCookTime" parameterType="edu.nf.food.food.label.entity.CookTime">
        insert into menu_cooking_time(menu_cooking_time)VALUES(#{name});
    </insert>
    <delete id="delCookTime" parameterType="edu.nf.food.food.label.entity.CookTime">
        delete from menu_cooking_time  where menu_cooking_time_id = #{id}
    </delete>
    <select id="listCookTime" resultMap="cooktime">
        select * from  menu_cooking_time
    </select>


</mapper>